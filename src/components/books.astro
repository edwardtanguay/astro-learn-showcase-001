---
import type { Book } from "../types";

const response = await fetch(
	"https://edwardtanguay.vercel.app/share/books.json"
);

let books: Book[] = [];

if (response.ok) {
	books = await response.json();
} else {
	console.log(response);
}
---

<section>
	{
		books.map((book: Book) => {
			return (
				<div>
					<div>{book.title}</div>
				</div>
			);
		})
	}
</section>
